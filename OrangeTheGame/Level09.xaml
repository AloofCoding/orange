        <Rectangle x:Name="rect_09" Fill="Black" HorizontalAlignment="Center" Height="20" Margin="366,160,406,239" Stroke="White" VerticalAlignment="Center" Width="20"/>
        <Rectangle x:Name="rect_08" Fill="Black" HorizontalAlignment="Center" Height="20" Margin="386,160,386,239" Stroke="White" VerticalAlignment="Center" Width="20"/>
        <Rectangle x:Name="rect_07" Fill="Black" HorizontalAlignment="Center" Height="20" Margin="406,160,366,239" Stroke="White" VerticalAlignment="Center" Width="20"/>
