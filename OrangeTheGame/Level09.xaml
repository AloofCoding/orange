        <Rectangle x:Name="rect_01" Fill="Black" HorizontalAlignment="Center" Height="20" Margin="366,240,406,159" Stroke="White" VerticalAlignment="Center" Width="20"/>
        <Button x:Name="btn_finish" Content="Finish this" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="211,225,0,0" Click="btn_finish_Click"/>

        <!-- Concept for level 9:
        snake game, spawn 5 (or 9) white objects which shall be eaten
        snake dies when having contact with the edge and level restarts
        level ends if all fruits are eaten -->
        
    </Grid>
</Window>
